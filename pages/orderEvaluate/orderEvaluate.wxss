.orderEvaluate{
	height: 100vh;
	width: 100vw;
	background: #f9f9f9;
	font-size: 14px;
	color: #333;
}
.orderEvaluate_item{
	background:#f9f9f9;
	min-height: 100px;
}
.orderEvaluate_content{
	width: calc(100vw - 20px);
	margin-left: 10px;
	background: #fff;
}
.orderEvaluate_people{
	min-height: 90px;
	background: #fff;
	margin-top: 20px;
}
.orderEvaluate_people_title{
	height: 50px;
	line-height: 50px;
}
.orderEvaluate_people_title_image{
	width: 30px;
	height: 30px;
	margin-top: 10px;
	margin-left: 10px;
}
.orderEvaluate_people_title_image image{
	height: 30px;
	width: 30px;
}
.orderEvaluate_people_title_name{
	width: calc(100vw - 160px);
	margin-left: 10px;
	color: #96a1a6;
}
.orderEvaluate_people_title_desc{
	width: 80px;
	text-align: center;
	font-size: 12px;
	color: #eeeeee;
	margin-top: 13px;
	margin-right: 5px;
}
.orderEvaluate_people_grade{
	text-align: center;
	margin: 30px;
	margin-top: 10px;
}
.orderEvaluate_btn{
	margin: 10px 20px;
	margin-top: 100px;
}
.orderEvaluate_btn_active{
	color: #fff;
	background: #64f4d2;
}
/* #64f4d2 */
