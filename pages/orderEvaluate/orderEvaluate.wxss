.orderEvaluate_text_area{
	width: calc(100vw - 60px);
	margin-left: 10px;
	background: gary;
	height: 150px;
	padding: 10px;
	background: #eeeeee;
}
.orderEvaluate_people_text{
	text-align: center;
	color: #fdd21f;
}
