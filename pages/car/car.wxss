.car{
	height: 100vh;
	width: 100vw;
	color: #333;
	font-size: 14px;
}
.car_content{
	height: calc(100vh - 50px);
	width: calc(100vw - 20px);
	margin-left: 10px;
}
.car_content_list{
	height: 100px;
	padding: 10px 0;
	border-bottom: 1px solid #ebebeb;
}
.car_content_list_radio{
	height: 100%;
	width: 40px;
	line-height: 100%;
	text-align: left;
	padding-top: 40px;
}
.car_content_list_img{
	width: 100px;
	height: 100%;
	position: relative;
}
.car_content_list_img_empty{
	position: absolute;
	left: 10px;
	top: 31px;
	width: 80px;
	height: 37px;
	border: 1px solid #fff;
	color: #fff;
	line-height: 37px;
	text-align: center;
}
.car_content_list_img image{
	width: 100px;
	height: 100%;
}
.car_content_list_intro{
	margin-left: 10px;
	width: calc(100vw - 170px);
	height: 100%;
}
.car_content_list_intro_title{
	height: 30px;
	width: 100%;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.car_content_list_intro_price{
	height: 20px;
	width: 100%;
	color: red;
}
.car_content_list_intro_num{
	height: 30px;
	width: 100%;
	padding-top: 20px;
}
.car_content_list_intro_num_btn{
	height: 30px;
	width: 30px;
	line-height: 30px;
	text-align: center;
	border: 1px solid #ebebeb;
}
.car_content_list_intro_num_btn_first{
	margin-left: calc(100vw - 270px);
	border-top-left-radius: 3px;
	border-bottom-left-radius: 3px;
}
.car_content_list_intro_num_btn_second{
	border-top-right-radius: 3px;
	border-bottom-right-radius: 3px;
}

.car_content_list_intro_num_center{
	height: 30px;
	width: 40px;
	text-align: center;
	line-height: 30px;
	border: 1px solid #ebebeb;
	border-right: 0;
	border-left: 0;
}

.car_footer{
	height: 50px;
	border-top: 2px solid #ebebeb;
}
.car_footer_radio{
	margin-left: 10px;
}

.van-swipe-cell__left, .van-swipe-cell__right {
  display: inline-block;
  width: 65px;
  height: 120px;
  font-size: 15px;
  line-height: 120px;
  color: #fff;
  text-align: center;
  background-color: #f44;
}
.car_empty{
	height: 100vh;
	width: 100vw;
	line-height: 100vh;
	text-align: center;
	font-size: 24px;
	color: #eee;
}
