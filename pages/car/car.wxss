.car{
	height: 100vh;
	width: 100vw;
	color: #333;
	font-size: 14px;
}
.car_content{
	height: calc(100vh - 50px);
	background: blue;
	width: calc(100vw - 20px);
	margin-left: 10px;
}
.car_content_list{
	height: 100px;
	background: red;
}
.car_footer{
	height: 50px;
	border-top: 1px solid #ebebeb;
}
